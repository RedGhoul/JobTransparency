@model AJobBoard.Models.Data.ETLStatus

<section class="site-section" style="background-color: currentColor;padding-top:200px;">
    <div class="container">
        <div class="row mb-5 justify-content-center">
            <div class="col-md-7 text-center" style="padding-bottom: 10px;">
                <h5 class="section-title mb-2" style="color:white;">Create ETL Job</h5>
            </div>
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-4"></div>
                    <div class="col-md-4">

                        <form asp-action="Create">
                            <div class="form-group">
                                <label asp-for="Name" class="control-label"></label>
                                <input asp-for="Name" class="form-control" />
                                <span asp-validation-for="Name" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <input type="submit" value="Create" class="btn btn-primary" />
                            </div>
                        </form>
                        <div>
                            <a class="btn btn-primary" asp-action="Index">Back to ETL Jobs</a>
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                </div>

            </div>
        </div>
    </div>



</section>

